# Google Sheets API Configuration
# Get your Google Sheets API key from: https://console.developers.google.com/
VITE_GOOGLE_API_KEY=your_google_api_key_here

# Your Google Sheets document ID (found in the URL)
# Example: https://docs.google.com/spreadsheets/d/SPREADSHEET_ID/edit
VITE_SPREADSHEET_ID=your_spreadsheet_id_here

# Firebase Configuration
# Get these values from your Firebase project settings
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
VITE_FIREBASE_APP_ID=your_app_id_here

# Dexcom API Configuration
# Get these from your Dexcom Developer Portal
DEXCOM_CLIENT_ID=your_dexcom_client_id_here
DEXCOM_CLIENT_SECRET=your_dexcom_client_secret_here
DEXCOM_REDIRECT_URI=https://your-firebase-project.cloudfunctions.net/dexcomOAuthCallback
DEXCOM_USE_SANDBOX=false
